local Mouse = game.Players.LocalPlayer:GetMouse()
local Closed = true
Mouse.KeyDown:connect(function(key)
 
if key == "v" then
if Closed == true then
    Closed = false
        game.CoreGui["HuhWare"].Enabled = false
    else
        Closed = true
            game.CoreGui["HuhWare"].Enabled = true
        end
    end
end)

